  <div class="container">
    <p></p>
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h1><%= @profile["first_name"] %>&acute;s page</h1>
      </div>
    <div class="panel-body">
       <div class="container">
    <div class="row">
      <div class="col-md-6">
        <div class="row">
        <div class="col-md-12">
          <div class="col-md-4">
            <img height="150" width="150" src="<%= @profile["picture_url"] %>" alt="<%= @profile["first_name"] %>" class="img-thumbnail">
          </div>
          <div class="col-md-8">
            <div class="col-md-4">
              <p class="text-muted">Name: </p>
              <p class="text-muted">Lives in:</p>
              <p class="text-muted">Rating:</p>
            </div>
            <div class="col-md-8">
              <p><%= @profile["first_name"] %> <%= @profile["last_name"] %></p>
              <p><%= @profile["location"] %></p>
              <p>
                <span class="glyphicon glyphicon-star"></span>
                <span class="glyphicon glyphicon-star"></span>
                <span class="glyphicon glyphicon-star"></span>
                <span class="glyphicon glyphicon-star-empty"></span>
              </p>
            </div>
          </div>
        </div>
        </div>
        <p>&nbsp;</p>
        <p></p>
        <div class="row">
          <div class="btn-group btn-group-justified">
            <a href="#" class="btn btn-primary"># of Stories</a>
            <a href="#" class="btn btn-primary"># of Followers</a>
            <a href="#" class="btn btn-primary"># of Following</a>
          </div>
        </div>
    </div>
    <div class="col-md-6">
      <p>
        <h3>Storyfeed</h3>
      </p>
      <div class="list-group">
        <% @user_stories.each do |story| %>
        <a href="<%= story.audio_url %>" class='list-group-item'>
        <p>
          <h5><b><%= story.name %></b></h5>
        </p>
        <p>
          <%= story.description %>
        </p>
        <p>
          <img height="150" width="150" src="<%= story.picture_url %>" class="img-thumbnail">
        </p>
        </a>
        <% end %>
      </div>
   </div>

</div>
</div>
</div>



</div>
